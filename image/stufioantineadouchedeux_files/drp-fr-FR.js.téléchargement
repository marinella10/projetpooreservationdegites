function dateRangePicker_setRegional(){var regional={monthName:['Janvier','Février','Mars','Avril','Mai','Juin','Juillet','Août','Septembre','Octobre','Novembre','Décembre'],monthNameShort:['janv.','févr.','mars','avril','mai','juin','juil.','août','sept.','oct.','nov.','déc.'],dayName:['Lundi','Mardi','Mercredi','Jeudi','Vendredi','Samedi','Dimanche'],dayNameShort:['L','M','M','J','V','S','D'],placeholderStart:'Arrivée',placeholderEnd:'Départ',placeholderDuration:'Durée',dateFormat:'DD MMM YYYY',processFormat:'DD/MM/YYYY'};return regional;}